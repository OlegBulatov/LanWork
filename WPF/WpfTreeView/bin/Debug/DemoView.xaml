                        <l:DataTemplate.Triggers >
                             <!--l:DataTrigger Binding = "{l:Binding Source={l:StaticResource ColorSetter}, Path=ColorIsOn}" Value = "True" >
                                 <l:Setter TargetName = "columnWithTrigger" Property = "Background" Value = "Pink" />
                             </l:DataTrigger -->

                             <!--l:DataTrigger Binding = "{l:Binding ElementName=chColor, Path=IsChecked}" Value = "True" >
                                 <l:Setter TargetName = "columnWithTrigger" Property = "Background" Value = "Pink" />
                             </l:DataTrigger -->

