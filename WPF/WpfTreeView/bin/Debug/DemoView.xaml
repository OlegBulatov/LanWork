                   <l:TextBlock Name = "columnHeader" Text = "Биржа" TextWrapping = "Wrap" />
	</l:CheckBox-->


        <l:CheckBox Name="chColor" Margin="0,0,0,0" DataContext="{l:Binding Path=DataContext, RelativeSource={l:RelativeSource Mode=FindAncestor, AncestorType={x:Type l:Window}}}" IsChecked="{l:Binding Path=ColorIsOn}">
                   <l:TextBlock Name = "columnHeader" Text = "Биржа" TextWrapping = "Wrap" />
	</l:CheckBox>


                        <l:DataTemplate.Triggers >
                             <l:DataTrigger Binding = "{l:Binding ElementName=chColor, Path=IsChecked}" Value = "True" >
                                 <l:Setter TargetName = "columnHeader" Property = "Background" Value = "Yellow" />
                             </l:DataTrigger >
                        </l:DataTemplate.Triggers >

                   </l:DataTemplate>

              </dxg:GridColumn.HeaderTemplate>

              <dxg:GridColumn.CellTemplate >
