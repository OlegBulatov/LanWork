                                 <l:Setter TargetName = "columnWithTrigger" Property = "Background" Value = "Pink" />
                             </l:DataTrigger -->

			     <l:MultiDataTrigger>
                                <l:MultiDataTrigger.Conditions>
                                    <l:Condition Binding = "{l:Binding RelativeSource={l:RelativeSource Mode=FindAncestor, AncestorType={x:Type l:Window}}, Path=DataContext.ColorIsOn}" Value = "True" />
                                    <l:Condition Binding = "{ l:Binding RowData.EvenRow}" Value = "True" />
                                </l:MultiDataTrigger.Conditions>
                                <l:Setter TargetName = "columnWithTrigger" Property = "Background" Value = "Pink" />
                            </l:MultiDataTrigger>


                        </l:DataTemplate.Triggers >
